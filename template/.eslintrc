{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "ecmaFeatures": {
    "modules": true,
    "jsx": true
  },
  "rules": {
    "quotes": [1, "single"],
    "no-return-assign": 0,
    "no-multi-spaces": 0,
    "no-use-before-define": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": 0,
    "no-script-url": 0
  },
  "plugins": [ "react" ]
}
